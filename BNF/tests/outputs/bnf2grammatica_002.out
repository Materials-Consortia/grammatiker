%productions%
syntax = rule | rule syntax;
rule = "<" rule_name ">" "::=" expression ";";
expression = list | list "|" expression;
line_end = EOL;
list = term | term list;
term = literal | "<" rule_name ">";
literal = '"' text '"' | "'" text "'";
EOL = "\n";
text = character | character text;
rule_name = word;
